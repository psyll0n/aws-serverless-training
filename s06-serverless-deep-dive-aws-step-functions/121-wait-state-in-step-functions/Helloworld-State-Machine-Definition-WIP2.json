{
  "Comment": "A Hello World example of the Amazon States Language using a Pass state",
  "StartAt": "WaitforSomeTime",
  "States": {
    "WaitforSomeTime": {
      "Comment": "Wait state",
      "Type": "Wait",
      "SecondsPath": "$.WaitTime",
      "Next": "HelloWorld"
    },
    "HelloWorld": {
      "Comment": "Hello World Step",
      "Type": "Pass",
      "Result": "Hello World!",
      "End": true
    }
  }
}
